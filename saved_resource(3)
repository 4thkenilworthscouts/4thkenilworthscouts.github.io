    FlipClock.Lang.Arabic = {

      'years'   : '',
      'months'  : '',
      'days'    : '',
      'hours'   : '',
      'minutes' : '',
      'seconds' : ''

    };

    /* Create various aliases for convenience */

    FlipClock.Lang['ar']      = FlipClock.Lang.Arabic;
    FlipClock.Lang['ar-ar']   = FlipClock.Lang.Arabic;
    FlipClock.Lang['arabic']  = FlipClock.Lang.Arabic;

}(jQuery));
